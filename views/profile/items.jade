extends main

block profile
                    
    .col-xs-12.col-md-9.col-md-offset-3
        if items.length == 0
            .col-xs-12.text-center
                .alert.alert-info
                    h1 No hay elementos que mostrar :(
                    .pad-sm
        else
            each item in items
                .col-xs-12.col-md-6.col-lg-4
                    include ../components/cards
